<%=  render  'head'  %>

<div  id="container_banking_licences"  style="min-width:  310px;  height:  500px;  margin:  0  auto"></div>

<%=  javascript_tag  do  %>

$(function()  {
    initPage();
});
$(window).bind('page:change',  function()  {
    initPage();
});

function  initPage()  {
        var  yLabels  =  <%=  @labels.to_json.html_safe  %>;

        $('#container_banking_licences').highcharts({
                chart:  {
                        type:  'area'
                },
                title:  {
                        text:  '<%=  @title  %>'
                },
                xAxis:  {
                        allowDecimals:  false,
                        categories:  yLabels,
                        title:  {
                                text:  'Quarter'
                        }
                },
                yAxis:  {
                        allowDecimals:  false,
                        title:  {
                                text:  '<%=  @title  %>'
                        },
                        labels:  {
                                formatter:  function  ()  {
                                        return  this.value;
                                }
                        }
                },
                plotOptions:  {
                        area:  {
                                marker:  {
                                        enabled:  false,
                                        symbol:  'circle',
                                        radius:  2,
                                        states:  {
                                                hover:  {
                                                        enabled:  true
                                                }
                                        }
                                }
                        }
                },
                series:  [{
                        name:  'Local',
                        showInLegend:  false,
                        data:  <%=  @licences.to_json.html_safe  -%>
                }]
        });
}
<%  end  %>
